package com.golden.ship.po;

import java.io.Serializable;
/**
 * 船舶实体类
 * @author wusihao
 *
 */
public class Ship implements Serializable{

	private static final long serialVersionUID = 1L;
	private String shipid;//船舶id
	private String shipname;//船名
	private String tonnage;//吨位
	private String shiplong;//船长
	private String shipwidth;//船宽
	private String shipcapacity;//船容
	private String draught;//吃水
	private String hatchnumber;//舱口数
	private String registryport;//船籍港
	private String hullInsurance;//船舶保险
	private String businesscertificate;//船舶营业证路径
	private String airworthinesscertificate;//船舶适航证书路径
	private String internationalcertificate;//船舶国籍证书
	private String currentprovince;//所属省份
	private String currentcity;//所属城市
	private String currentport;//港口
	private String picturepath;//船图片地址
	private String createuser;//创建用户
	private String publishtime;//发布时间
	private String shiptype;//船类型
	private String auditstate;//审核状态  1:待审核 2:待修改 3:通过
	private String status;//状态  1:空闲 2:竞标中 3:货运中
	private String shipstatus;//(用户货物)是否申请船运 0未申请 1已申请
	
	
	public String getShipstatus() {
		return shipstatus;
	}
	public void setShipstatus(String shipstatus) {
		this.shipstatus = shipstatus;
	}
	public String getShipid() {
		return shipid;
	}
	public void setShipid(String shipid) {
		this.shipid = shipid;
	}
	public String getShipname() {
		return shipname;
	}
	public void setShipname(String shipname) {
		this.shipname = shipname;
	}
	
	public String getRegistryport() {
		return registryport;
	}
	public void setRegistryport(String registryport) {
		this.registryport = registryport;
	}
    public String getTonnage() {
        return tonnage;
    }
    public void setTonnage(String tonnage) {
        this.tonnage = tonnage;
    }
    public String getShiplong() {
        return shiplong;
    }
    public void setShiplong(String shiplong) {
        this.shiplong = shiplong;
    }
    public String getShipwidth() {
        return shipwidth;
    }
    public void setShipwidth(String shipwidth) {
        this.shipwidth = shipwidth;
    }
    public String getShipcapacity() {
        return shipcapacity;
    }
    public void setShipcapacity(String shipcapacity) {
        this.shipcapacity = shipcapacity;
    }
    public String getDraught() {
        return draught;
    }
    public void setDraught(String draught) {
        this.draught = draught;
    }
    public String getHatchnumber() {
		return hatchnumber;
	}
	public void setHatchnumber(String hatchnumber) {
		this.hatchnumber = hatchnumber;
	}
	public String getHullInsurance() {
		return hullInsurance;
	}
	public void setHullInsurance(String hullInsurance) {
		this.hullInsurance = hullInsurance;
	}
	public String getBusinesscertificate() {
		return businesscertificate;
	}
	public void setBusinesscertificate(String businesscertificate) {
		this.businesscertificate = businesscertificate;
	}
	public String getAirworthinesscertificate() {
		return airworthinesscertificate;
	}
	public void setAirworthinesscertificate(String airworthinesscertificate) {
		this.airworthinesscertificate = airworthinesscertificate;
	}
	public String getInternationalcertificate() {
		return internationalcertificate;
	}
	public void setInternationalcertificate(String internationalcertificate) {
		this.internationalcertificate = internationalcertificate;
	}
	public String getPicturepath() {
		return picturepath;
	}
	public void setPicturepath(String picturepath) {
		this.picturepath = picturepath;
	}
	public String getCreateuser() {
		return createuser;
	}
	public void setCreateuser(String createuser) {
		this.createuser = createuser;
	}
	public String getShiptype() {
		return shiptype;
	}
	public void setShiptype(String shiptype) {
		this.shiptype = shiptype;
	}
	public String getAuditstate() {
		return auditstate;
	}
	public void setAuditstate(String auditstate) {
		this.auditstate = auditstate;
	}
	public String getStatus() {
		return status;
	}
	public void setStatus(String status) {
		this.status = status;
	}
	public String getPublishtime() {
		return publishtime;
	}
	public void setPublishtime(String publishtime) {
		this.publishtime = publishtime;
	}
    public String getCurrentprovince() {
        return currentprovince;
    }
    public void setCurrentprovince(String currentprovince) {
        this.currentprovince = currentprovince;
    }
    public String getCurrentcity() {
        return currentcity;
    }
    public void setCurrentcity(String currentcity) {
        this.currentcity = currentcity;
    }
    public String getCurrentport() {
        return currentport;
    }
    public void setCurrentport(String currentport) {
        this.currentport = currentport;
    }
}
